{
	"name": "Skogen",
	"type": "dark",
	"colors": {
		"editor.background": "#2a2a26",
		"editor.foreground": "#f200fa",
		"activityBarBadge.background": "#FF8445",
		"sideBarTitle.foreground": "#D0D0D2"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#515b62"
			}
		},
		{
			"name": "YAML keys",
			"scope": [
				"constant",
				"entity.name.function",
				"entity.name.tag.tsx",
				"entity.name.tag",
				"punctuation.definition.heading.markdown",
				"punctuation.definition.list.begin.markdown",
				"punctuation.definition.markdown",
				"punctuation.definition.metadata.markdown",
				"punctuation.definition.raw.markdown",
				"text.html.markdown",
			],
			"settings": {
				"foreground": "#A1A1A3"
			}
		},
		{
			"name": "TODO:",
			"scope": [
				"entity.other.attribute-name.class.css",
				"punctuation.definition.string.begin",
				"punctuation.definition.string.end",
				"storage.type",
				"string.other",
				"string.quoted",
				"string.template",
				"string.unquoted",
			],
			"settings": {
				"foreground": "#4E683E"
			}
		},
		{
			"name": "Number",
			"scope": [
				"constant.numeric",
			],
			"settings": {
				"foreground": "#ff7a7a"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"meta.brace",
				"meta.jsx.children.tsx",
				"punctuation",
			],
			"settings": {
				"foreground": "#D0D0D2"
			}
		},
		{
			"name": "Orange",
			"scope": [
				"keyword.control",
				"keyword.operator",
				"keyword.other.unit",
				"punctuation.support.type.property-name",
				"source.json meta.structure.dictionary.json support.type.property-name.json",
				"storage.modifier",
				"punctuation.definition.keyword"
			],
			"settings": {
				"foreground": "#D0883E"
			}
		},
		{
			"name": "Lighter green",
			"scope": [
				"constant.other.timestamp",
				"meta.enum.declaration.ts",
				"meta.tag.attributes.tsx",
				"punctuation.definition.variable",
				"variable",
			],
			"settings": {
				"foreground": "#7D9863"
			}
		},
		{
			"name": "TYPES DEFS",
			"scope": [
				"entity.name.type",
				"entity.other.inherited-class",
				"meta.object-literal.key",
				"meta.object.member",
				"source.ignore",
			],
			"settings": {
				"foreground": "#FFF7D0"
			}
		},
		{
			"name": "TYPES Primitives & errors & JSX",
			"scope": [
				"support.class.component.tsx",
				"support.class.error.ts",
				"support.constant.property-value",
				"support.type.object.module",
				"support.type.primitive",
				"support.type.property-name",
				"support.function",
			],
			"settings": {
				"foreground": "#FF8445"
			}
		}
	]
}
